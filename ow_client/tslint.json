{
  "extends": [
    "tslint-react"
  ],
  "rules": {
    "ordered-imports": [
      false
    ],
    "comment-format": [
      false
    ],

    "member-access": [
      false
    ],
    "member-ordering": [
      true
    ],
    "no-empty": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-constant-condition": false,
    "triple-equals": [
      true,
      "allow-undefined-check"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-duplicate-imports": false,
    "jsx-alignment": false,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": false
  }
}