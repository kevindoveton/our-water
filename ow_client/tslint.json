{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "ordered-imports": [
      false
    ],
    "comment-format": [
      false
    ],
    // "quotemark": [
    //   true,
    //   "single",
    //   "double",
    //   "jsx-single",
    //   "avoid-escape"
    // ],
    "member-access": [
      false
    ],
    "member-ordering": [
      false
    ],
    // "trailing-comma": [
    //   true,
    //   {
    //     "singleline": "never",
    //     "multiline": "never"
    //   }
    // ],
    "no-empty": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-constant-condition": false,
    "triple-equals": [
      true,
      "allow-undefined-check"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-duplicate-imports": false,
    "jsx-alignment": false,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "max-classes-per-file": false,
    "interface-name": false
  }
}